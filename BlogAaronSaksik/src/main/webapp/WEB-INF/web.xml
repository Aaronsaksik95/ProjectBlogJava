<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>BlogAaronSaksik</display-name>
  <jsp-config>
    <jsp-property-group>
        <url-pattern>*.jsp</url-pattern>
        <page-encoding>UTF-8</page-encoding>
    </jsp-property-group>
</jsp-config>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>ynov.aaron.servlet.Index</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>AllArticle</servlet-name>
        <servlet-class>ynov.aaron.servlet.AllArticle</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>AddArticle</servlet-name>
        <servlet-class>ynov.aaron.servlet.AddArticle</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>UpdateArticle</servlet-name>
        <servlet-class>ynov.aaron.servlet.UpdateArticle</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>DeleteArticle</servlet-name>
        <servlet-class>ynov.aaron.servlet.DeleteArticle</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>OneArticle</servlet-name>
        <servlet-class>ynov.aaron.servlet.OneArticle</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>Auteur</servlet-name>
        <servlet-class>ynov.aaron.servlet.Auteur</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>AllArticle</servlet-name>
        <url-pattern>/AllArticle</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>AddArticle</servlet-name>
        <url-pattern>/AddArticle</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>UpdateArticle</servlet-name>
        <url-pattern>/UpdateArticle</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>DeleteArticle</servlet-name>
        <url-pattern>/DeleteArticle</url-pattern>
    </servlet-mapping>
    
  	<servlet-mapping>
        <servlet-name>OneArticle</servlet-name>
        <url-pattern>/OneArticle</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>Auteur</servlet-name>
        <url-pattern>/Auteur</url-pattern>
    </servlet-mapping>
    
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>Index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>